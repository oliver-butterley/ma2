{
    "cSpell.words": [
        "Apostol",
        "Bolzano",
        "Butterley",
        "Canestrari",
        "chktex",
        "extremum",
        "Morrey",
        "Protter",
        "resp",
        "Reuleaux",
        "Schwarz",
        "Vergata",
        "Weierstrass"
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "--shell-escape",
                "-file-line-error",
                "-pdf",        
                "-outdir=%OUTDIR%",
                "-output-directory=build",
                "%DOC%"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.outDir": "%DIR%/build",
    "cSpell.enableFiletypes": [
        "!yaml"
    ],
    "cSpell.language": "en-GB",
    "cSpell.ignorePaths": [
        "**/*.md",
        ".vscode"
    ],
    "latex-workshop.linting.chktex.enabled": true,
    "latex-workshop.linting.chktex.args.root": [
        "-wall",
        "-n22",
        "-n30",
        "-e16",
        "-n21",
        "-n15",
        "-17",
        "-q",
        "-l ../mychktexrc"
    ],
    "latex-workshop.linting.chktex.args.active": [
        "-wall",
        "-n22",
        "-n30",
        "-e16",
        "-n21",
        "-n15",
        "-n17",
        "-q"
    ],
    "latex-workshop.linting.chktex.exec.args": [
        "-wall",
        "-n22",
        "-n30",
        "-e16",
        "-q",
        "-n21"
    ]
}