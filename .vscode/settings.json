{
    "cSpell.words": [
        "Apostol",
        "Bolzano",
        "extremum",
        "resp",
        "Reuleaux",
        "Schwarz",
        "Weierstrass"
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "--shell-escape",
                "-file-line-error",
                "-pdf",        
                "-outdir=%OUTDIR%",
                "-output-directory=build",
                "%DOC%"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.outDir": "%DIR%/build",
    "cSpell.enableFiletypes": [
        "!yaml"
    ]
}